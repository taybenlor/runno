{
  "name": "@runno/wasi-motor",
  "version": "0.3.0",
  "author": "Ben Taylor <runno@taybenlor.com>",
  "description": "new WASI implementation for @runno",
  "website": "https://runno.dev",
  "repository": {
    "type": "git",
    "url": "git://github.com/taybenlor/runno.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build:esm": "esbuild src/index.ts --bundle --format=esm --outfile=dist/index.esm.js",
    "build:cjs": "esbuild src/index.ts --bundle --format=cjs --outfile=dist/index.js",
    "build:types": "tsc",
    "build": "npm run build:esm && npm run build:cjs && npm run build:types"
  },
  "devDependencies": {
    "esbuild": "^0.14.48",
    "typescript": "^4.4.4",
    "tslib": "^2.2.0"
  },
  "dependencies": {}
}
