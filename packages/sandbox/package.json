{
  "name": "@runno/sandbox",
  "version": "0.7.0",
  "description": "Run untrusted code inside a WebAssembly sandbox.",
  "keywords": [
    "sandbox",
    "python",
    "ruby",
    "wasi",
    "wasm"
  ],
  "author": "Ben Taylor <me@taybenlor.com>",
  "homepage": "https://runno.dev",
  "license": "MIT",
  "type": "module",
  "main": "lib/main.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "langs/*.tar.gz",
    "langs/*.wasm"
  ],
  "bin": {
    "runno": "bin/main.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taybenlor/runno.git"
  },
  "scripts": {
    "test": "deno test --no-check --allow-net",
    "cli": "deno --allow-net --allow-read src/main.ts"
  },
  "bugs": {
    "url": "https://github.com/taybenlor/runno/issues"
  },
  "dependencies": {
    "@cliffy/command": "npm:@jsr/cliffy__command@^1.0.0-rc.7",
    "@runno/wasi": "^0.7.0",
    "@std/tar": "npm:@jsr/std__tar@^0.1.3"
  }
}
