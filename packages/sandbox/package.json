{
  "name": "@runno/sandbox",
  "version": "0.7.0",
  "description": "Run code inside the Runno WebAssembly sandbox.",
  "keywords": [
    "wasm",
    "sandbox"
  ],
  "author": "Ben Taylor <me@taybenlor.com>",
  "homepage": "https://runno.dev",
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taybenlor/runno.git"
  },
  "scripts": {
    "test": "echo \"No tests yet!\"",
    "build": "tsup"
  },
  "bugs": {
    "url": "https://github.com/taybenlor/runno/issues"
  },
  "dependencies": {
    "@cliffy/command": "npm:@jsr/cliffy__command@^1.0.0-rc.7",
    "@runno/wasi": "^0.7.1",
    "@std/tar": "npm:@jsr/std__tar@^0.1.3"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  }
}
