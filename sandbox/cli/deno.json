{
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@runno/wasi": "npm:@runno/wasi@^0.7.0",
    "@std/tar": "jsr:@std/tar@^0.1.3",
    "@std/assert": "jsr:@std/assert@^1.0.10",
    "@std/io": "jsr:@std/io@^0.225.0"
  },
  "compilerOptions": {
    "lib": ["ESNext", "deno.window"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
  "tasks": {
    "test": "deno test --allow-read",
    "cli": "deno --allow-net --allow-read src/main.ts",
    "bootstrap": "cp -R ../../langs .",
    "compile": "deno compile --frozen --include langs --output runno bin/main.ts"
  },
  "lock": {
    "path": "./deno.lock",
    "frozen": true
  }
}
